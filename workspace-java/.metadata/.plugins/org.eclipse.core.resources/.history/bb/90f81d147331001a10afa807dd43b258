package uri1160;
import java.util.Scanner;

public class uri1160 {
	public static void main(String agrs[]) {
		
		Scanner teclado;
		teclado = new Scanner(System.in);
		
		int t=1, ANOS;
		int PA, PB;
		double G1, G2;
		
		t = teclado.nextInt();
		ANOS = 0;
		while (t > 0) {

			PA = teclado.nextInt();
			PB = teclado.nextInt();
			G1 = teclado.nextDouble();
			G2 = teclado.nextDouble();
			
			
			
			while (PA <= PB){
				
				PA+= PA*(G1/100.0);
				PB+= PB*(G2/100.0);

				ANOS++;
				
				
			}
			
			t = t - 1;
			
		}
		if (ANOS > 100){
			System.out.println("Mais de 1 seculo.");
			
		}else {
			if (ANOS <= 100)
				System.out.printf("%d anos.\n",ANOS);
			
		}
		teclado.close();
	}

}
